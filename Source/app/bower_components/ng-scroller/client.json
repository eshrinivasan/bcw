{
  "apps.dist": {
    "output": "./dist/ng-scroller.min.js",
    "output.deps": "./dist/deps.js",
    "namespaces": [ "ng.Scroller.module" ],
    "roots": [
      "./lib"
    ],

    "closure-compiler.flags.angular_pass": true
  },

  "apps.facebook": {
    "output.deps": "./examples/build/facebook-deps.js",
    "namespaces": [ "app" ],
    "roots": [
      "./examples/facebook",
      "./lib"
    ]
  },

  "apps.twitter": {
    "output.deps": "./examples/build/twitter-deps.js",
    "namespaces": [ "app" ],
    "roots": [
      "./examples/twitter",
      "./lib"
    ]
  },

  "externs.angular": "./externs/angular.js",

  "closure-compiler": "./build/closure-compiler",
  "closure-library": "./lib/closure-library"
}
